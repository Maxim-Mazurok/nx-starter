{
  "path": ".",
  "tag-prefix": "website-v",
  "releaseCommitMessageFormat": "chore(release): @nx-starter/website@{{currentTag}}",
  "compareUrlFormat": "https://nx-starter.domain.cpm/branchCompare?baseVersion=GT{{previousTag}}&targetVersion=GT{{currentTag}}",
  "issueUrlFormat": "https://nx-starter.atlassian.net/browse/{{id}}",
  "parserOpts": {
    "headerCorrespondence": [
      "type",
      "scope",
      "subject",
      "body"
    ]
  },
  "types": [
    {"type": "feat", "section": "Features"},
    {"type": "fix", "section": "Bug fixes"},
    {"type": "style", "section": "Styling"},
    {"type": "test", "section": "Testing changes"},
    {"type": "refactor", "section": "Refactoring changes"},
    {"type": "chore", "section": "Development changes"},
    {"type": "docs", "section": "Documentations"}
  ]
}
